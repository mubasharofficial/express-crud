        <div class="container">
            <div class="row mt-5">
                <div class="col-sm-8 col-md-offset-2">
                    <h1 class="alert alert-success text-center">Edit Updated Student</h1>
                    <form method="post" action="/student/update/<%=data._id;%>">
                        <div class="mb-3">
                            <label class="form-label" for="name">Name</label>
                            <input class="form-control" type="text" id="name" name="name" value="<%=data.name %>">
                        </div>
                        <div class="mb-3">
                            <label class="form-label" for="age">Age</label>
                            <input class="form-control" type="text" id="age" name="age" value="<%=data.age%>">
                        </div>

                        <div class="mb-3">
                            <label class="form-label" for="fee">Fee</label>
                            <input class="form-control" type="text" id="fee" name="fee" value="<%=data.fess%>">
                        </div>

                        <button type="submit" class="btn btn-warning">Submit</button> 
                        <button type="submit" class="btn btn-secondary">BACK HOME</button> 

                    </form>
                </div>
            </div>
        </div>


